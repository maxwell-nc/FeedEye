<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pres.nc.maxwell.feedeye"
    android:versionCode="1"
    android:versionName="测试版本" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="16" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name="pres.nc.maxwell.feedeye.application.FeedEyeApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="pres.nc.maxwell.feedeye.activity.SplashActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="pres.nc.maxwell.feedeye.activity.MainActivity" >
        </activity>
        <activity android:name="pres.nc.maxwell.feedeye.activity.SearchItemActivity" >
        </activity>
        <activity android:name="pres.nc.maxwell.feedeye.activity.AddFeedActivity" >
        </activity>
    </application>

</manifest>